<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="EatIt">
      <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    	
    	  <class>com.db.entities.Client</class>
    	  <class>com.db.entities.Databasemanager</class>
    	  <class>com.db.entities.Favourite</class>
    	  <class>com.db.entities.Feedback</class>
    	  <class>com.db.entities.Reservation</class>
    	  <class>com.db.entities.Restaurant</class>
    
   	  
    	  
 
      <properties>
 <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
 <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/eatitall"/>
 <property name="javax.persistence.jdbc.user" value="root"/>
 <property name="javax.persistence.jdbc.password" value="admin"/>
      </properties>
	</persistence-unit>
</persistence>
